{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "appshell",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ],
        "urls": [
          "https://fonts.googleapis.com/css?family=Roboto:300,400,500",
          "https://fonts.googleapis.com/icon?family=Material+Icons"
        ]
      }
    }, 
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "dynamic",
      "urls":[
        "/car-service",
        "/car-service/**",
        "/cars",
        "/appointments",
        "/me"
      ],
      "cacheConfig": {
        "maxSize": 5,
        "maxAge": "3h",
        "timeout": "10s",
        "strategy": "performance"
      }
    }
  ]
}
